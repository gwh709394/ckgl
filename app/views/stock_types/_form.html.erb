<%= simple_form_for(@stock_type) do |f| %>
  <%= f.error_notification %>

  <div class="form-group">
    <%= f.input :name, input_html: { class: 'form-control'}, label: '名称'%>
    <%= f.input :s_type, collection: StockingType.menu,  input_html: { class: 'form-control'}, label: '名称' %>
  </div>

  <div class="form-group">
    <%= f.submit '保存',  class: 'btn btn-primary' %>
    <%= link_to '返回', stock_types_path %>
  </div>
<% end %>
