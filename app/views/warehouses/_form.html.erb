<%= simple_form_for(warehouse) do |f| %>
  <%= f.error_notification %>
  <div class="form-group">
    <%= f.input :name, input_html: { class: 'form-control'}, label: '名称' %>
  </div>
  <div class="form-group">
    <%= f.input :code, input_html: { class: 'form-control'}, label: '代码' %>
  </div>
  <div class="form-group">
    <%= f.input :location, input_html: { class: 'form-control'}, label: '地点' %>
  </div>
  <div class="form-group">
    <%= f.input :remark, input_html: { class: 'form-control'}, label: '备注' %>
  </div>

  <div class="form-group">
    <%= f.submit '保存', class: 'btn btn-primary' %>
    <%= link_to '返回', warehouses_path %>
  </div>
<% end %>
